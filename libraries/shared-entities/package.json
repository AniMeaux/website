    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/lodash.intersection": "^4.4.6",
    "@types/uuid": "^8.3.3",
    "babel-plugin-macros": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4"
