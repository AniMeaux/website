{
  "name": "@animeaux/faune-remix",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "msw": {
    "workerDirectory": "./public"
  },
  "scripts": {
    "dev": "node -r dotenv-flow/config ./node_modules/.bin/remix dev",
    "build": "run-s \"build:css --minify\" \"build:icons --minify\" build:theme build:remix",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./src/generated/tailwind.css",
    "build:icons": "ts-node --files ./scripts/generateIconSprite.ts",
    "build:theme": "ts-node --files ./scripts/generateTheme.ts",
    "build:remix": "remix build",
    "start": "remix-serve ./build",
    "clean": "rm -rf ./build ./public/build ./src/generated",
    "clean-build": "run-s clean build"
  },
  "dependencies": {
    "@animeaux/shared": "*",
    "@prisma/client": "^3.11.1",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/serve": "^1.6.4",
    "cookie-signature": "^1.2.0",
    "focus-visible": "^5.2.0",
    "history": "^5.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.4",
    "@types/cookie-signature": "^1.0.4",
    "@types/node": "^17.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "dotenv-flow": "^3.2.0",
    "msw": "^0.41.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
