{
  "name": "@animeaux/show",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "msw": {
    "workerDirectory": "./public"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "build:icons": "ts-node --files ./scripts/generateIconSprite.ts",
    "build:image-shapes": "ts-node --files ./scripts/generateImageShapeSprite.ts",
    "build:pictograms": "ts-node --files ./scripts/generatePictogramSprite.ts",
    "build:remix": "remix build",
    "build:theme": "ts-node --files ./scripts/generateTheme.ts",
    "build": "run-s \"build:icons --minify\" \"build:image-shapes --minify\" \"build:pictograms --minify\" build:theme build:remix",
    "clean-build": "run-s clean build",
    "clean": "rm -rf ./build ./public/build ./src/generated",
    "dev": "PORT=3001 node -r dotenv-flow/config ./node_modules/.bin/remix dev --port 3011",
    "generate": "run-s \"build:icons --minify\" \"build:image-shapes --minify\" \"build:pictograms --minify\" build:theme",
    "script": "NODE_OPTIONS=\"-r dotenv-flow/config\" ts-node --files",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@animeaux/core": "*",
    "@animeaux/form-data": "*",
    "@animeaux/zod-utils": "*",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "~1.19.3",
    "@remix-run/react": "~1.19.3",
    "@remix-run/serve": "~1.19.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@unpic/placeholder": "^0.1.2",
    "cachified": "^3.5.4",
    "cloudinary": "^1.40.0",
    "focus-visible": "^5.2.0",
    "isbot": "^3.6.13",
    "lodash.chunk": "^4.2.0",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lru-cache": "^10.0.1",
    "luxon": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "~1.19.3",
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.orderby": "^4.6.7",
    "@types/luxon": "^3.3.1",
    "@types/node": "^17.0.6",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@unpic/pixels": "^1.2.0",
    "autoprefixer": "^10.4.15",
    "blurhash": "^2.0.5",
    "dotenv-flow": "^3.3.0",
    "msw": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  }
}
