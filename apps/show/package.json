{
  "name": "@animeaux/show",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "msw": {
    "workerDirectory": "./public"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "generate": "run-p --continue-on-error --print-label generate:*",
    "generate:icons": "tsx ./scripts/generate-icon-sprite.ts",
    "generate:pictograms": "tsx ./scripts/generate-pictogram-sprite.ts",
    "generate:theme": "tsx ./scripts/generate-theme.ts",
    "predev": "yarn generate",
    "dev": "run-p --continue-on-error --print-label dev:*",
    "dev:icons": "yarn generate:icons --watch",
    "dev:pictograms": "yarn generate:pictograms --watch",
    "dev:remix": "node -r dotenv-flow/config ./node_modules/.bin/remix vite:dev --host",
    "dev:theme": "tsx watch --clear-screen=false ./scripts/generate-theme.ts",
    "build": "run-s build:icons build:pictograms build:theme build:remix",
    "build:icons": "yarn generate:icons --minify",
    "build:pictograms": "yarn generate:pictograms --minify",
    "build:remix": "remix vite:build",
    "build:theme": "yarn generate:theme",
    "start": "remix-serve ./build/server/index.js",
    "script": "NODE_OPTIONS=\"-r dotenv-flow/config\" tsx"
  },
  "dependencies": {
    "@animeaux/core": "*",
    "@animeaux/react-primitives": "*",
    "@animeaux/search-params-io": "*",
    "@animeaux/zod-utils": "*",
    "@epic-web/cachified": "^5.2.0",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/node": "~2.9.2",
    "@remix-run/react": "~2.9.2",
    "@remix-run/serve": "~2.9.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@unpic/placeholder": "^0.1.2",
    "cloudinary": "^2.2.0",
    "isbot": "^5.1.8",
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.partition": "^4.6.0",
    "lru-cache": "^10.0.1",
    "luxon": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remix-utils": "^7.5.0",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "~2.9.2",
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.orderby": "^4.6.7",
    "@types/lodash.partition": "^4.6.9",
    "@types/luxon": "^3.3.1",
    "@types/node": "^22.8.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@unpic/pixels": "^1.2.0",
    "autoprefixer": "^10.4.15",
    "blurhash": "^2.0.5",
    "chokidar": "^3.5.3",
    "dotenv-flow": "^4.1.0",
    "msw": "^2.3.1",
    "node-html-parser": "^6.1.12",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.7.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
