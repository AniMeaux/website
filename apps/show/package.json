{
  "name": "@animeaux/show",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "msw": {
    "workerDirectory": "./public"
  },
  "scripts": {
    "build:icons": "ts-node --files ./scripts/generateIconSprite.ts",
    "build:image-shapes": "ts-node --files ./scripts/generateImageShapeSprite.ts",
    "build:pictograms": "ts-node --files ./scripts/generatePictogramSprite.ts",
    "build:remix": "remix build",
    "build:theme": "ts-node --files ./scripts/generateTheme.ts",
    "build": "run-s \"build:icons --minify\" \"build:image-shapes --minify\" \"build:pictograms --minify\" build:theme build:remix",
    "clean-build": "run-s clean build",
    "clean": "rm -rf ./build ./public/build ./src/generated",
    "dev": "node -r dotenv-flow/config ./node_modules/.bin/remix dev",
    "generate": "run-s \"build:icons --minify\" \"build:image-shapes --minify\" \"build:pictograms --minify\" build:theme",
    "script": "NODE_OPTIONS=\"-r dotenv-flow/config\" ts-node --files",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@radix-ui/react-primitive": "^1.0.2",
    "@remix-run/node": "~1.16.0",
    "@remix-run/react": "~1.16.0",
    "@remix-run/serve": "~1.16.0",
    "@unpic/placeholder": "^0.1.2",
    "focus-visible": "^5.2.0",
    "lodash.orderby": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "~1.16.0",
    "@types/lodash.orderby": "^4.6.6",
    "@types/node": "^17.0.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@unpic/pixels": "^1.2.0",
    "blurhash": "^2.0.5",
    "dotenv-flow": "^3.2.0",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  }
}
