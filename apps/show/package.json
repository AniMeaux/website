{
  "name": "@animeaux/show",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "msw": {
    "workerDirectory": "./public"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "prebuild": "pnpm clean && pnpm generate",
    "build": "run-s --print-label build:server build:app",
    "build:app": "remix vite:build",
    "build:server": "tsc -p ./tsconfig.server.json",
    "clean": "rm -rf ./build ./public/build ./src/generated ./server.js",
    "predev": "pnpm generate",
    "dev": "run-p --continue-on-error --print-label dev:*",
    "dev:app": "NODE_OPTIONS=\"-r dotenv-flow/config\" remix vite:dev --host",
    "dev:icons": "pnpm generate:icons --watch",
    "dev:pictograms": "pnpm generate:pictograms --watch",
    "dev:theme": "tsx watch --clear-screen=false ./scripts/generate-theme.ts",
    "generate": "run-p --continue-on-error --print-label generate:*",
    "generate:icons": "tsx ./scripts/generate-icon-sprite.ts --minify",
    "generate:pictograms": "tsx ./scripts/generate-pictogram-sprite.ts --minify",
    "generate:theme": "tsx ./scripts/generate-theme.ts",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:types": "tsc --noEmit",
    "script": "NODE_OPTIONS=\"-r dotenv-flow/config\" tsx",
    "start": "node ./server.js ./build/server/index.js"
  },
  "dependencies": {
    "@animeaux/core": "workspace:*",
    "@animeaux/file-storage": "workspace:*",
    "@animeaux/files-io": "workspace:*",
    "@animeaux/prisma": "workspace:*",
    "@animeaux/react-primitives": "workspace:*",
    "@animeaux/search-params-io": "workspace:*",
    "@animeaux/zod-utils": "workspace:*",
    "@conform-to/react": "catalog:",
    "@conform-to/zod": "catalog:",
    "@epic-web/cachified": "catalog:",
    "@mjackson/form-data-parser": "catalog:",
    "@mjackson/lazy-file": "catalog:",
    "@radix-ui/react-collapsible": "catalog:",
    "@radix-ui/react-dialog": "catalog:",
    "@radix-ui/react-navigation-menu": "catalog:",
    "@radix-ui/react-popover": "catalog:",
    "@react-email/components": "catalog:",
    "@react-email/render": "catalog:",
    "@remix-run/express": "catalog:",
    "@remix-run/node": "catalog:",
    "@remix-run/react": "catalog:",
    "@sentry/remix": "catalog:",
    "@unpic/pixels": "catalog:",
    "@unpic/placeholder": "catalog:",
    "blurhash": "catalog:",
    "cloudinary": "catalog:",
    "compression": "catalog:",
    "express": "catalog:",
    "get-port": "catalog:",
    "isbot": "catalog:",
    "lodash.chunk": "catalog:",
    "lodash.groupby": "catalog:",
    "lodash.orderby": "catalog:",
    "lru-cache": "catalog:",
    "luxon": "catalog:",
    "morgan": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-markdown": "catalog:",
    "remark-breaks": "catalog:",
    "remark-gfm": "catalog:",
    "remix-utils": "catalog:",
    "resend": "catalog:",
    "scroll-into-view-if-needed": "catalog:",
    "source-map-support": "catalog:",
    "tailwindcss": "catalog:",
    "tiny-invariant": "catalog:",
    "uuid": "catalog:"
  },
  "devDependencies": {
    "@remix-run/dev": "catalog:",
    "@total-typescript/ts-reset": "catalog:",
    "@types/compression": "catalog:",
    "@types/express": "catalog:",
    "@types/lodash.chunk": "catalog:",
    "@types/lodash.groupby": "catalog:",
    "@types/lodash.orderby": "catalog:",
    "@types/luxon": "catalog:",
    "@types/morgan": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/source-map-support": "catalog:",
    "@types/uuid": "catalog:",
    "autoprefixer": "catalog:",
    "chokidar": "catalog:",
    "dotenv-flow": "catalog:",
    "msw": "catalog:",
    "node-html-parser": "catalog:",
    "npm-run-all": "catalog:",
    "tsx": "catalog:",
    "type-fest": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "catalog:"
  }
}
