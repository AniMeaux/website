    "date-fns": "^2.16.1",
    "fs-extra": "^9.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
