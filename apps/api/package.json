    "firebase-admin": "^9.4.2",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.7.0",
    "lodash.isempty": "^4.4.0",
    "lodash.orderby": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
