{
  "name": "@animeaux/admin",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "msw": {
    "workerDirectory": "./public"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "prebuild": "pnpm clean && pnpm generate",
    "build": "run-s --print-label build:app",
    "build:app": "remix vite:build",
    "clean": "rm -rf ./build ./public/build ./src/generated",
    "predev": "pnpm generate",
    "dev": "run-p --continue-on-error --print-label dev:*",
    "dev:app": "NODE_OPTIONS=\"-r dotenv-flow/config\" remix vite:dev --host",
    "dev:icons": "pnpm generate:icons --watch",
    "dev:theme": "tsx watch --clear-screen=false ./scripts/generate-theme.ts",
    "generate": "run-p --continue-on-error --print-label generate:*",
    "generate:icons": "tsx ./scripts/generate-icon-sprite.ts --minify",
    "generate:theme": "tsx ./scripts/generate-theme.ts",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:types": "tsc --noEmit",
    "start": "remix-serve ./build/server/index.js"
  },
  "dependencies": {
    "@animeaux/core": "workspace:*",
    "@animeaux/file-storage": "workspace:*",
    "@animeaux/form-data": "workspace:*",
    "@animeaux/password": "workspace:*",
    "@animeaux/prisma": "workspace:*",
    "@animeaux/react-primitives": "workspace:*",
    "@animeaux/search-params-io": "workspace:*",
    "@animeaux/zod-utils": "workspace:*",
    "@conform-to/react": "catalog:",
    "@conform-to/zod": "catalog:",
    "@radix-ui/react-dialog": "catalog:",
    "@radix-ui/react-dropdown-menu": "catalog:",
    "@radix-ui/react-popover": "catalog:",
    "@radix-ui/react-visually-hidden": "catalog:",
    "@remix-run/node": "catalog:",
    "@remix-run/react": "catalog:",
    "@remix-run/serve": "catalog:",
    "@sentry/remix": "catalog:",
    "autosize": "catalog:",
    "bowser": "catalog:",
    "cloudinary": "catalog:",
    "cookie-signature": "catalog:",
    "croner": "catalog:",
    "d3-dsv": "catalog:",
    "downshift": "catalog:",
    "history": "catalog:",
    "isbot": "catalog:",
    "lodash.chunk": "catalog:",
    "lodash.difference": "catalog:",
    "lodash.intersection": "catalog:",
    "lodash.isequal": "catalog:",
    "lodash.merge": "catalog:",
