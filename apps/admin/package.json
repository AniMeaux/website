{
  "name": "@animeaux/admin",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "msw": {
    "workerDirectory": "./public"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "generate": "run-p --continue-on-error --print-label generate:*",
    "generate:icons": "tsx ./scripts/generate-icon-sprite.ts",
    "generate:theme": "tsx ./scripts/generate-theme.ts",
    "predev": "yarn generate",
    "dev": "run-p --continue-on-error --print-label dev:*",
    "dev:icons": "yarn generate:icons --watch",
    "dev:remix": "node -r dotenv-flow/config ./node_modules/.bin/remix vite:dev --host",
    "dev:theme": "tsx watch --clear-screen=false ./scripts/generate-theme.ts",
    "build": "run-s build:icons build:theme build:remix",
    "build:icons": "yarn generate:icons --minify",
    "build:remix": "remix vite:build",
    "build:theme": "yarn generate:theme",
    "start": "remix-serve ./build/server/index.js"
  },
  "dependencies": {
    "@algolia/client-search": "^4.22.1",
    "@animeaux/algolia-client": "*",
    "@animeaux/core": "*",
    "@animeaux/form-data": "*",
    "@animeaux/password": "*",
    "@animeaux/react-primitives": "*",
    "@animeaux/search-params-io": "*",
    "@animeaux/zod-utils": "*",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@remix-run/node": "~2.9.2",
    "@remix-run/react": "~2.9.2",
    "@remix-run/serve": "~2.9.2",
    "algoliasearch": "^4.19.1",
    "autosize": "^6.0.1",
    "bowser": "^2.11.0",
    "cloudinary": "^2.2.0",
    "cookie-signature": "^1.2.1",
    "croner": "^8.0.0",
    "downshift": "^9.0.6",
    "history": "^5.3.0",
    "isbot": "^5.1.8",
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.zip": "^4.2.0",
    "luxon": "^3.4.2",
    "metascraper": "^5.36.0",
    "metascraper-date": "^5.36.0",
    "metascraper-image": "^5.36.0",
    "metascraper-publisher": "^5.36.0",
    "metascraper-title": "^5.36.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remix-utils": "^7.5.0",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@animeaux/tailwind-animation": "*",
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "~2.9.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/autosize": "^4.0.1",
    "@types/cookie-signature": "^1.1.0",
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.intersection": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.orderby": "^4.6.7",
    "@types/lodash.zip": "^4.2.7",
    "@types/luxon": "^3.3.1",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.15",
    "chokidar": "^3.5.3",
    "dotenv-flow": "^4.1.0",
    "msw": "^2.3.1",
    "node-html-parser": "^6.1.12",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.7.0",
    "type-fest": "^4.3.1",
    "typescript": "~5.4.5",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
