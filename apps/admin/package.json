    "metascraper-publisher": "catalog:",
    "metascraper-title": "catalog:",
    "react": "catalog:",
    "react-dnd": "catalog:",
    "react-dnd-html5-backend": "catalog:",
    "react-dnd-touch-backend": "catalog:",
    "react-dom": "catalog:",
    "react-markdown": "catalog:",
    "react-router": "catalog:",
    "remark-breaks": "catalog:",
    "remark-gfm": "catalog:",
    "remix-utils": "catalog:",
    "tiny-invariant": "catalog:",
    "uuid": "catalog:"
  },
  "devDependencies": {
    "@animeaux/tailwind-animation": "workspace:*",
    "@faker-js/faker": "catalog:",
    "@remix-run/dev": "catalog:",
    "@tailwindcss/container-queries": "catalog:",
    "@total-typescript/ts-reset": "catalog:",
    "@types/autosize": "catalog:",
