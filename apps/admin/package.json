{
  "name": "@animeaux/admin",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "msw": {
    "workerDirectory": "./public"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./src/generated/tailwind.css",
    "build:icons": "ts-node --files ./scripts/generateIconSprite.ts",
    "build:remix": "remix build",
    "build:theme": "ts-node --files ./scripts/generateTheme.ts",
    "build": "run-s \"build:css --minify\" \"build:icons --minify\" build:theme build:remix",
    "clean-build": "run-s clean build",
    "clean": "rm -rf ./build ./public/build ./src/generated",
    "dev": "node -r dotenv-flow/config ./node_modules/.bin/remix dev",
    "generate": "run-s \"build:css --minify\" \"build:icons --minify\" build:theme",
    "start": "remix-serve ./build"
  },
  "dependencies": {
    "@algolia/client-search": "^4.14.2",
    "@animeaux/shared": "*",
    "@prisma/client": "^4.8.0",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-primitive": "^1.0.2",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-visually-hidden": "^1.0.2",
    "@remix-run/node": "~1.16.0",
    "@remix-run/react": "~1.16.0",
    "@remix-run/serve": "~1.16.0",
    "algoliasearch": "^4.14.2",
    "autosize": "^5.0.1",
    "bowser": "^2.11.0",
    "cloudinary": "^1.33.0",
    "cookie-signature": "^1.2.0",
    "downshift": "^7.0.4",
    "focus-visible": "^5.2.0",
    "history": "^5.3.0",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.zip": "^4.2.0",
    "luxon": "^2.3.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^7.1.2",
    "react-transition-group": "^4.4.2",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remix-utils": "^6.0.0",
    "tiny-invariant": "^1.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.17.3",
    "zod-form-data": "^1.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@remix-run/dev": "~1.16.0",
    "@types/cookie-signature": "^1.0.4",
    "@types/lodash.difference": "^4.5.6",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.zip": "^4.2.7",
    "@types/node": "^17.0.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^9.0.0",
    "dotenv-flow": "^3.2.0",
    "msw": "^0.49.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "type-fest": "^3.6.0",
    "typescript": "~4.9.4"
  }
}
