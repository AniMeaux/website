    "@types/luxon": "^3.3.1",
    "@types/node": "^17.0.6",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.15",
    "dotenv-flow": "^3.3.0",
    "msw": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "type-fest": "^4.3.1",
    "typescript": "~5.2.2"
  }
}
