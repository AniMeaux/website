{
  "name": "@animeaux/prisma",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/src/index.d.ts",
      "default": "./build/src/index.js"
    },
    "./server": {
      "types": "./build/src/index.server.d.ts",
      "default": "./build/src/index.server.js"
    }
  },
  "scripts": {
    "prebuild": "pnpm clean && pnpm generate",
    "build": "run-s --print-label build:src build:paths",
    "build:paths": "tsc-alias -p ./tsconfig.build.json",
    "build:src": "tsc -p ./tsconfig.build.json",
    "clean": "rm -rf ./build ./src/generated",
    "copy-data": "tsx ./scripts/copy-data.ts",
    "dev": "run-p --continue-on-error --print-label dev:*",
    "dev:paths": "pnpm build:paths --watch",
    "dev:prisma": "chokidar ./schema.prisma -c 'pnpm generate:prisma'",
    "dev:src": "pnpm build:src --watch",
    "download-all-data": "tsx -r dotenv-flow/config ./scripts/download-all-data.ts",
    "generate": "run-p --continue-on-error --print-label generate:*",
    "generate:prisma": "prisma generate",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:types": "tsc --noEmit",
    "migrate": "yarn prisma-env migrate dev",
    "prisma-env": "node -r dotenv-flow/config ./node_modules/.bin/prisma",
    "reset": "yarn prisma-env migrate reset -f",
    "studio": "yarn prisma-env studio"
  },
  "prisma": {
    "seed": "tsx -r dotenv-flow/config ./scripts/seed-data.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "catalog:",
    "@prisma/client": "catalog:",
    "prisma": "catalog:"
  },
  "devDependencies": {
    "@animeaux/password": "workspace:*",
    "@faker-js/faker": "catalog:",
    "@types/d3-dsv": "catalog:",
    "@types/luxon": "catalog:",
    "@types/pg": "catalog:",
    "@types/pg-copy-streams": "catalog:",
    "chokidar-cli": "catalog:",
    "d3-dsv": "catalog:",
    "dotenv-flow": "catalog:",
    "luxon": "catalog:",
    "npm-run-all": "catalog:",
    "pg": "catalog:",
    "pg-copy-streams": "catalog:",
    "tiny-invariant": "catalog:",
    "tsc-alias": "catalog:",
    "tsx": "catalog:",
    "type-fest": "catalog:",
    "typescript": "catalog:"
  }
}
